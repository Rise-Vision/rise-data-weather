{
  "name": "rise-data-weather",
  "version": "1.0.0",
  "description": "Web component for retrieving weather data on a Rise Vision Template page",
  "scripts": {
    "prebuild": "eslint . && ./node_modules/rise-common-component/scripts/create_config.sh prod rise-data-weather",
    "build": "polymer build && ./node_modules/rise-common-component/scripts/extract-source.sh rise-data-weather",
    "pretest": "eslint . && ./node_modules/rise-common-component/scripts/create_config.sh test rise-data-weather",
    "test": "polymer test",
    "pretty": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rise-Vision/rise-data-weather.git"
  },
  "keywords": [
    "Rise",
    "Vision",
    "Digital",
    "Signage",
    "weather"
  ],
  "author": "Rise Vision",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Rise-Vision/rise-data-weather/issues"
  },
  "homepage": "https://github.com/Rise-Vision/rise-data-weather#readme",
  "dependencies": {
    "@polymer/polymer": "3.1.0",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "rise-common-component": "git://github.com/Rise-Vision/rise-common-component.git#feature/fetch-mixin"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "chai": "^4.2.0",
    "eslint": "^5.6.1",
    "eslint-config-idiomatic": "^4.0.0",
    "mocha": "^5.2.0",
    "sinon": "^7.3.2",
    "wct-mocha": "^1.0.0"
  }
